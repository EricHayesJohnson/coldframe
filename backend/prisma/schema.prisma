generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SensorReading {
  id           Int      @id @default(autoincrement())
  createdAt    DateTime @default(now())
  timestamp    DateTime
  sensorId     String
  temperatureC Float
  humidityPct  Float
  pressureHPa  Float
}
